{
  "expo": {
    "name": "HiveManager",
    "slug": "HiveManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imgs/LogoSVG.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/imgs/LogoSVG.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/imgs/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/imgs/favicon.png"
    }
  }
}
